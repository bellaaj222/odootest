<odoo>
    <record id="view_web_diagram_plus_tree" model="ir.ui.view">
        <field name="model">web.diagram.plus</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="view_web_diagram_plus_form" model="ir.ui.view">
        <field name="model">web.diagram.plus</field>
        <field name="arch" type="xml">
            <form>
                <field name="id"/>
                <sheet>
                    <group>
                        <div class="oe_button_box" name="button_box">
                            <button name="action_web_diagram_plus" type="object" string="View Diagram"
                                    class="oe_stat_button" icon="fa-code-fork"></button>


                        </div>
                    </group>

                        <div class="oe_title">
                            <h1 name="header_type">
                                   <field name="name" placeholder="Diagram name"/>
                            </h1>
                        </div>
                    <group>

                        <notebook>

                            <page string="DÃ©partments">

                                <field name="node_ids"/>
                            </page>
                            <page string="Actions WorkFlow">
                                <field name="arrow_ids"/>
                            </page>
                        </notebook>
                    </group>
                </sheet>
            </form>
        </field>

    </record>
    <record id="view_web_diagram_plus" model="ir.ui.view">
        <field name="model">web.diagram.plus</field>
        <field name="type">diagram_plus</field>
        <field name="arch" type="xml">
            <diagram_plus auto_layout="false">
                <node object="web.diagram.plus.node" bgcolor="gray:closed" bg_color_field="res_bg_color"
                      fg_color_field="res_label_color" d_position_field="diagram_position" label="['name']">

                </node>
                <arrow object="web.diagram.plus.arrow" source="from_node_id" destination="to_node_id"
                       label="['actions','date','index']"></arrow>
            </diagram_plus>
        </field>
    </record>
    <record id="action_web_diagram_plus_model" model="ir.actions.act_window">
        <field name="name">Web Diagram Plus Model</field>
        <field name="res_model">web.diagram.plus</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem name="Web Diagram " id="web_diagram_plus_parent_menu_item" sequence="220" parent="crnd_web_tests"/>
    <menuitem name="Web Diagram " id="web_diagram_plus_menu_item" sequence="10"
              parent="web_diagram_plus_parent_menu_item" action="action_web_diagram_plus_model"/>
</odoo>