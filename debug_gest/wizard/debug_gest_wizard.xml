<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_debug_gest_wizard_form" model="ir.ui.view">
        <field name="name">debug.gest.wizard.form</field>
        <field name="model">debug.gest.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="dev_ids" widget="many2many_avatar_employee"/>
                        <!--                            # TODO : add domain -->
                    </group>
                    <group>
                        <field name="note"/>
                    </group>
                </group>
                <footer>
                    <button name="action_affect" string="Affecter" type="object" class="btn-primary"/>
                    <button special="cancel" string="Annuler" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>